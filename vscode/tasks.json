        {
            "label": "Build",
            "type": "shell",
            "command": "npm",
            "args": [
                "run",
                "build"
            ],
            "problemMatcher": [],
            "group": "build"
        },
        {
            "label": "Test",
            "type": "shell",
            "command": "npm",
            "args": [
                "test"
            ],
            "group": {
                "kind": "test",
                "isDefault": true
            }
        }
    ]
}
